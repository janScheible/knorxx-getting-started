<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<action>
		<actionName>run</actionName>
		<packagings>
			<packaging>jar</packaging>
		</packagings>
		<goals>
			<goal>spring-boot:run</goal>
		</goals>
	</action>
	<action>
		<actionName>debug</actionName>
		<packagings>
			<packaging>jar</packaging>
		</packagings>
		<goals>
			<goal>spring-boot:run</goal>
		</goals>
		<properties>
			<jpda.listen>maven</jpda.listen>
			<Env.SPRING_BOOT_DEBUGGING>true</Env.SPRING_BOOT_DEBUGGING>
		</properties>
		<activatedProfiles>
			<activatedProfile>spring-boot-maven-plugin-debug</activatedProfile>
		</activatedProfiles>
	</action>
</actions>
